+size = 48x48
+
+themedir = $(datadir)/icons/hicolor
+iconsdir = $(themedir)/$(size)/apps
+
+icons_DATA = session-properties.png
+
+gtk_update_icon_cache = gtk-update-icon-cache -f -t $(datadir)/icons/hicolor
+
